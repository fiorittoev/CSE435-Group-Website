source "https://rubygems.org"

gem "jekyll", "~> 4.3.0"
gem "webrick", "~> 1.8"
# Ensure stdlib gems that are no longer bundled by default on some Ruby installs
# are available to Jekyll on Windows / newer Ruby versions
gem "csv"
gem "logger"
gem "base64"
gem "jekyll-feed"
gem "jekyll-seo-tag"

group :jekyll_plugins do
  gem "jekyll-feed"
  gem "jekyll-seo-tag"
end